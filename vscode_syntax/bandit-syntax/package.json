{
    "name": "bandit-syntax",
    "displayName": "Bandit Syntax",
    "description": "Syntax highlighting for .bdt files",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "bdt",
            "aliases": ["Bandit", "bdt"],
            "extensions": [".bdt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bdt",
            "scopeName": "source.bdt",
            "path": "./syntaxes/bdt.tmLanguage.json"
        }]
    }
}